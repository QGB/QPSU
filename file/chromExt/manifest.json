{
  "name": "QGB chromExt",
  "description": "RPC",
  "version": "0",
  "permissions": [
    "activeTab",
	"tabs",
	"contextMenus",
	"storage",
	"cookies" ,
	"bookmarks",
	"<all_urls>"	
  ],

  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"  ,
  "background": {
    "scripts": ["qgb.js"]
  },
  "browser_action": {
    "default_title": "QGB",
	"default_popup": "popup.html"
  },
  "manifest_version": 2
}
